    "url3": "assets/mobile/RoadRipper_Images/2)ColorPalette_RoadRippers.webp",
    "url4": "assets/mobile/RoadRipper_Images/3)Banner_RoadRippers.webp",
    "p4": "2018 branding refresh.",
    "url5": "assets/mobile/RoadRipper_Images/4)Poster_RoadRippers.webp",
